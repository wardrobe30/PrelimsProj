<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.edwardjohn.prelimsproj" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Departments"
            android:label="@string/title_activity_departments"
            android:parentActivityName=".MyActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.edwardjohn.prelimsproj.MyActivity" />
        </activity>
        <activity
            android:name=".IT"
            android:label="@string/title_activity_it"
            android:parentActivityName=".Departments" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.edwardjohn.prelimsproj.Departments" />
        </activity>
        <activity
            android:name=".CS"
            android:label="@string/title_activity_cs"
            android:parentActivityName=".Departments" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.edwardjohn.prelimsproj.Departments" />
        </activity>
        <activity
            android:name=".IS"
            android:label="@string/title_activity_is"
            android:parentActivityName=".Departments" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.edwardjohn.prelimsproj.Departments" />
        </activity>
    </application>

</manifest>
